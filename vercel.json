{
  "version": 2,
  "functions": {
    "backend/main.py": {
      "runtime": "vercel-python@0.1.0"
    }
  },
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/frontend/static/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/backend/main.py"
    },
    {
      "source": "/((?!api|static).*)",
      "destination": "/frontend/index.html"
    }
  ]
}
