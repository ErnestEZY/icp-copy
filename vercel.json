{
  "version": 2,
  "functions": {
    "api/*.py": {
      "maxDuration": 15
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "api/index.py"
    },
    {
      "source": "/static/(.*)",
      "destination": "frontend/static/$1"
    },
    {
      "source": "/favicon.ico",
      "destination": "frontend/static/favicon-32x32.png"
    },
    {
      "source": "/(.*)",
      "destination": "frontend/index.html"
    }
  ]
}
